@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

skinparam classAttributeIconSize 0
hide circle
hide methods

class Customer {
    Customer_ID
    NIF
    Name
    Address
    Mobile_number
    Email
    type_ID
    Country_ID
}

class Product {
    Product_ID
    Product_Name
    Factory_Plant_ID
    Market_Demand
    Optimization
    Production_Cost
    Flexibility
    Family_ID
}

class Factory_Plant {
    Factory_Plant_ID
    Safety
    Efficiency
    Space
}

class Country {
    Country_ID
    Country_Name
}

class Type {
    Type_ID
    Name
}

class Customer_Order {
    Customer_Order_ID
    Order_Date
    Delivery_Date
    Location
    Customer_ID
}

class BOO {
    Operation_ID
    Product_ID
    Next_Operation_ID
}

class Operation {
    Operation_ID
    Operation_Type_ID
    Execution_Time
}

class Operation_Type {
    Operation_Type_ID
    Operation_Description
}

class Part {
    Part_ID
    Part_Description
}

class Supplier {
    Supplier_ID
    Supplier_Name
}

class Procurement {
    Procurement_ID
    Part_ID
    Supplier_ID
    Price
    Min_Qnt
    Start_Date
    End_Date
}

class Stock {
    Part_ID
    Stock
    Unit
}

class Reserved_Stock {
    Reserved_ID
    Part_ID
    Quantity
    Unit
}

class Product_Family {
    Family_ID
    Family_Description
}

Customer "1" -- "0..*" Customer_Order: has >
Customer "1" -- "1" Type: has >
Customer "1" -- "0..*" Country: has >

Customer_Order "1" -- "0..*" Product: has >

Product "1" -- "0..*" BOO: has >

Product "1" -- "1" Factory_Plant: is produced in >

Product "1" -- "1" Product_Family : belongs to >

Factory_Plant "1" -- "0..*" Operation: has >

Operation "1" -- "0..*" BOO : belongs to >
Operation "1" -- "1" Operation_Type : has >
BOO "1" -- "0..*" Part : has >
Part "1" -- "0..*" Stock : has >
Stock "1" --- "0..*" Reserved_Stock : has >
Procurement "1" -- "1" Supplier : has >
Procurement "1" -- "0..*" Part : has >
Reserved_Stock "1" --- "1" Part : has >
Supplier "1" -- "0..*" Part : has >


@enduml
